{
  "name": "graphai-discord-bot-supabase",
  "version": "1.0.0",
  "description": "GraphAI-powered Discord Bot with Multimodal Capabilities, using Supabase",
  "type": "commonjs",
  "main": "dist/index.js",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "npx tsc",
    "dev": "npx tsc --watch & node --watch dist/index.js",
    "lint": "eslint . --ext .ts",
    "prepare": "npm run build",
    "diagnose": "npx tsc fix-bot.ts && node fix-bot.js",
    "start:fixed": "npx tsc && node dist/index-fix.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "@supabase/supabase-js": "^2.38.4",
    "cross-blob": "^3.0.2",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "graphai": "^0.5.0",
    "js-yaml": "^4.1.0",
    "node-fetch": "^2.7.0",
    "undici": "^5.28.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "typescript": "^5.3.3"
  }
}